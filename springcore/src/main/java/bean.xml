<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    
    
    
	<bean class="com.te.springcore.bean.Employee" id="employee" autowire="byName">
		<property name="empId" value="100"></property>
		<property name="eName" value="Rahul"></property>
		<!-- <property name="Address" ref="address"></property> -->
		
	</bean>
	<bean class="com.te.springcore.bean.Address" id="address" >
		<property name="hNo" value="4/951-c" />
		<property name="street" value="Vennampatty Road" />
		<property name="country" value="Tamilnadu"></property>
	</bean>





</beans>